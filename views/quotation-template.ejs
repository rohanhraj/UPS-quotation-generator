<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>ARVI Power - Quotation</title>
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;600;700&display=swap');

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        html,
        body {
            height: 100%;
        }

        body {
            font-family: 'Open Sans', Arial, sans-serif;
            font-size: 11px;
            line-height: 1.4;
            color: #333;
            background: white;
            display: flex;
            flex-direction: column;
        }

        .container {
            max-width: 210mm;
            width: 100%;
            margin: 0 auto;
            padding: 10mm;
            flex: 1;
            display: flex;
            flex-direction: column;
        }

        .footer-pusher {
            flex: 1;
        }

        /* Enforce uniform font sizes */
        p,
        li,
        .data-table,
        .quote-table,
        .terms-table,
        .tech-box {
            font-size: 11px !important;
        }

        .blue-header,
        .section-title,
        .option-header,
        .company-footer h4,
        .option-title .model {
            font-size: 12px !important;
        }

        /* Header */
        .header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding-bottom: 6px;
            border-bottom: 1px solid #ddd;
            margin-bottom: 8px;
        }

        .header-left {
            display: flex;
            align-items: center;
            gap: 8px;
        }

        .year-badge {
            width: 40px;
            height: 40px;
            background: linear-gradient(135deg, #c9a961 0%, #b8956a 100%);
            border-radius: 50%;
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            color: white;
        }

        .year-badge .num {
            font-size: 16px;
            font-weight: 700;
            line-height: 1;
        }

        .year-badge .text {
            font-size: 4px;
            text-align: center;
        }

        .header-info {
            font-size: 9px;
            color: #666;
            max-width: 250px;
        }

        .header-info .iso {
            font-weight: 600;
            font-size: 8px;
        }

        .logo-img {
            height: 35px;
        }

        /* Blue Headers */
        .blue-header {
            background: #5a6a7a;
            color: white;
            text-align: center;
            padding: 4px 8px;
            font-weight: 600;
            font-size: 12px;
            margin: 8px 0 6px;
        }

        /* Red Accent */
        .red {
            color: #c41e3a;
        }

        .red-bold {
            color: #c41e3a;
            font-weight: 700;
        }

        /* Reference Box */
        .ref-box {
            background: #f8f8f8;
            border: 1px solid #e0e0e0;
            padding: 6px 10px;
            margin: 6px 0;
        }

        .ref-box p {
            margin: 2px 0;
            font-size: 11px;
        }

        /* Section Title */
        .section-title {
            font-size: 12px;
            font-weight: 600;
            margin: 8px 0 5px;
        }

        /* Content Row */
        .content-row {
            display: flex;
            gap: 10px;
            margin: 4px 0;
            align-items: flex-start;
        }

        .img-col {
            flex: 0 0 110px;
        }

        .img-col img {
            width: 100%;
            height: auto;
            border: 1px solid #ddd;
            margin-bottom: 4px;
        }

        .img-col img:last-child {
            margin-bottom: 0;
        }

        .text-col {
            flex: 1;
            text-align: justify;
            line-height: 1.4;
        }

        .text-col p {
            margin-bottom: 3px;
        }

        .text-col p:first-child {
            margin-top: 0;
        }

        /* Bullet List */
        ul.bullets {
            padding-left: 12px;
            margin: 2px 0;
            list-style-type: none;
        }

        ul.bullets li {
            margin-bottom: 1px;
            position: relative;
            padding-left: 15px;
        }

        ul.bullets li::before {
            content: "";
            position: absolute;
            left: 0;
            color: #c41e3a;
            font-weight: bold;
        }

        /* Image Row */
        .img-row {
            display: flex;
            gap: 6px;
            margin: 6px 0;
        }

        .img-row img {
            flex: 1;
            height: 70px;
            object-fit: cover;
            border: 1px solid #ddd;
        }

        /* Stats Row */
        .stats-row {
            display: flex;
            justify-content: center;
            gap: 50px;
            margin: 10px 0;
            text-align: center;
        }

        .stat .number {
            font-size: 20px;
            font-weight: 700;
            color: #c41e3a;
        }

        .stat .label {
            font-size: 8px;
            color: #666;
        }

        /* Tables */
        .data-table {
            width: 100%;
            border-collapse: collapse;
            margin: 6px 0;
        }

        .data-table th,
        .data-table td {
            border: 1px solid #999;
            padding: 3px 6px;
            text-align: left;
        }

        .data-table th {
            background: #5a6a7a;
            color: white;
            font-weight: 600;
            font-size: 9px;
        }

        .data-table td:first-child {
            font-weight: 500;
            width: 35%;
        }

        /* Quote Table */
        .quote-table {
            width: 100%;
            border-collapse: collapse;
            margin: 6px 0;
        }

        .quote-table th,
        .quote-table td {
            border: 1px solid #999;
            padding: 4px 6px;
        }

        .quote-table th {
            background: #e8e8e8;
            font-weight: 600;
            text-align: center;
        }

        .quote-table td {
            vertical-align: top;
        }

        .quote-table td:first-child {
            text-align: center;
            width: 35px;
        }

        .quote-table td:nth-child(3),
        .quote-table td:nth-child(4),
        .quote-table td:nth-child(5) {
            text-align: center;
        }

        /* Option Header */
        .option-header {
            background: #5a6a7a;
            color: white;
            text-align: center;
            padding: 4px;
            font-weight: 600;
            font-size: 12px;
            margin: 10px 0 5px;
        }

        .option-title {
            text-align: center;
            margin: 4px 0;
        }

        .option-title .model {
            color: #c41e3a;
            font-weight: 700;
            font-size: 12px;
        }

        .option-title .desc {
            font-size: 11px;
        }

        /* Terms Table */
        .terms-table {
            width: 100%;
            border-collapse: collapse;
            margin: 6px 0;
        }

        .terms-table th {
            background: #5a6a7a;
            color: white;
            padding: 4px;
            font-weight: 600;
        }

        .terms-table td {
            border: 1px solid #999;
            padding: 3px 6px;
            vertical-align: top;
        }

        .terms-table td:first-child {
            width: 60px;
            font-weight: 600;
            background: #f5f5f5;
        }

        /* Footer */
        .footer {
            margin-top: 15px;
        }

        .company-footer {
            background: #d3e4f0;
            padding: 6px 10px;
            text-align: center;
        }

        .company-footer h4 {
            color: #2c5282;
            font-size: 12px;
            margin-bottom: 2px;
        }

        .company-footer p {
            font-size: 9px;
            color: #333;
        }

        .cert-footer {
            background: #f8f8f8;
            padding: 4px 10px;
            display: flex;
            justify-content: center;
            align-items: center;
            gap: 8px;
            border-top: 1px solid #ddd;
        }

        .cert-footer img {
            height: 22px;
        }

        .cert-footer span {
            font-size: 8px;
        }

        /* Quote Box */
        .quote-box {
            background: #f5f5f5;
            border-left: 3px solid #5a6a7a;
            padding: 6px 10px;
            margin: 6px 0;
            font-size: 8px;
        }

        .quote-box p {
            margin-bottom: 4px;
        }

        /* Principles Box */
        .principles-box {
            display: flex;
            gap: 10px;
            margin: 6px 0;
            align-items: flex-start;
        }

        .shield-icon {
            flex: 0 0 40px;
            text-align: center;
        }

        .shield-placeholder {
            width: 35px;
            height: 35px;
            background: #5a6a7a;
            border-radius: 6px;
            display: flex;
            align-items: center;
            justify-content: center;
            color: white;
            font-size: 16px;
        }

        /* Motto Box */
        .motto-box {
            display: flex;
            gap: 10px;
            margin: 6px 0;
        }

        .motto-label {
            flex: 0 0 50px;
            background: #5a6a7a;
            color: white;
            padding: 6px;
            text-align: center;
            font-weight: 600;
            font-size: 10px;
        }

        /* Technical Details Box */
        .tech-box {
            background: #f9f9f9;
            padding: 6px 10px;
            margin: 6px 0;
        }

        .tech-box p {
            margin-bottom: 4px;
        }

        /* Signature Section */
        .signature-section {
            margin: 12px 0;
            font-size: 9px;
        }

        .signature-section p {
            margin-bottom: 4px;
        }

        /* Horizontal Line */
        hr.section-divider {
            border: none;
            border-top: 2px solid #5a6a7a;
            margin: 10px 0;
        }

        /* Page Break Control */
        .page-break {
            page-break-before: always;
        }

        .avoid-break {
            page-break-inside: avoid;
        }

        /* Total Section */
        .total-section {
            margin: 10px 0;
            background: linear-gradient(135deg, #f8f9fa 0%, #e9ecef 100%);
            border-radius: 6px;
            padding: 8px 12px;
            border: 1px solid #ddd;
        }

        .total-row {
            display: flex;
            justify-content: flex-end;
            align-items: center;
            padding: 4px 0;
            border-bottom: 1px solid #e0e0e0;
        }

        .total-row:last-child {
            border-bottom: none;
        }

        .total-label {
            font-size: 9px;
            font-weight: 500;
            color: #666;
            min-width: 80px;
            text-align: right;
            margin-right: 12px;
        }

        .total-value {
            font-size: 10px;
            font-weight: 600;
            color: #333;
            min-width: 100px;
            text-align: right;
        }

        .total-row.grand-total {
            background: #c41e3a;
            margin: 6px -12px -8px;
            padding: 8px 12px;
            border-radius: 0 0 6px 6px;
        }

        .total-row.grand-total .total-label {
            color: rgba(255, 255, 255, 0.9);
            font-size: 10px;
            font-weight: 600;
        }

        .total-row.grand-total .total-value {
            color: white;
            font-size: 12px;
            font-weight: 700;
        }

        @media print {
            body {
                background: white;
                -webkit-print-color-adjust: exact;
                print-color-adjust: exact;
            }

            html,
            body {
                height: 100%;
            }

            .container {
                width: 100%;
                max-width: none;
                padding: 0;
                min-height: 100%;
                display: flex;
                flex-direction: column;
            }

            .footer-pusher {
                flex: 1;
            }

            @page {
                size: A4;
                margin: 10mm;
            }
        }
    </style>
</head>

<body>
    <div class="container">
        <!-- HEADER -->
        <div class="header">
            <div class="header-left">
                <div class="year-badge">
                    <span class="num">27</span>
                    <span class="text">Years of<br>Excellence</span>
                </div>
                <div class="header-info">
                    <div class="iso">ISO 9001:2015</div>
                    <div>Rated as the BEST In-House R&D among Indian UPS manufacturers by SD.</div>
                </div>
            </div>
            <% if (typeof isBase64 !=='undefined' && isBase64 && assetImages && assetImages['IMG-20260205-WA0005.jpg'])
                { %>
                <img src="<%= assetImages['IMG-20260205-WA0005.jpg'] %>" alt="ARVI Power" class="logo-img">
                <% } else { %>
                    <img src="<%= assetsPath %>/IMG-20260205-WA0005.jpg" alt="ARVI Power" class="logo-img">
                    <% } %>
        </div>

        <!-- REFERENCE BOX -->
        <div class="ref-box">
            <p><strong>Quotation No:</strong> <%= data.quoteNumber || '' %> &nbsp;&nbsp; <strong>Date:</strong> <%= data.quoteDate || new Date().toISOString().split('T')[0] %></p>
            <p><strong>To:</strong> <%= data.customerName || '' %><% if (data.customerAddress) { %>, <%= data.customerAddress %><% } %></p>
            <% if (data.kindAttn) { %><p><strong>Kind Attn:</strong> <%= data.kindAttn %></p><% } %>
            <% if (data.referenceText) { %>
                <div style="margin-top: 8px; font-size: 11px;"><%- data.referenceText %></div>
            <% } %>
        </div>

        <!-- SECTION 1: What sets ARVI apart -->
        <div class="blue-header">What sets Arvi apart from the rest? Let's take a closer look.</div>

        <div class="section-title">1. Truly Indian with <span class="red-bold">75 - 80% local content/ Indian components</span></div>

        <div class="content-row avoid-break">
            <div class="img-col">
                <% if (typeof isBase64 !=='undefined' && isBase64 && assetImages &&
                    assetImages['IMG-20260205-WA0004.jpg']) { %>
                    <img src="<%= assetImages['IMG-20260205-WA0004.jpg'] %>" alt="UPS Internal">
                    <% } else { %>
                        <img src="<%= assetsPath %>/IMG-20260205-WA0004.jpg" alt="UPS Internal">
                        <% } %>
                            <% if (typeof isBase64 !=='undefined' && isBase64 && assetImages &&
                                assetImages['IMG-20260205-WA0010.jpg']) { %>
                                <img src="<%= assetImages['IMG-20260205-WA0010.jpg'] %>" alt="UPS Cabinet">
                                <% } else { %>
                                    <img src="<%= assetsPath %>/IMG-20260205-WA0010.jpg" alt="UPS Cabinet">
                                    <% } %>
            </div>
            <div class="text-col">
                <p>At a time when self-reliance is driving India‚Äôs industrial backbone, Arvi stands out with a 
                    <strong>75 ‚Äì 80% locally designed and manufactured product line</strong>. This indigenous approach not only 
                    ensures robust quality but also strengthens India‚Äôs vision of <strong>‚Äú Make in India.‚Äù</strong>
                </p>

                <p>Many <strong>so called manufacturers import</strong> UPS as semi knocked down kits and reassemble it in 
                    India under Make in India initiative; but the UPS design and its sub assembly are not Indian. They 
                    are designed for general requirement and are mass produced and sold as "box selling" and hence the 
                    life of these UPS are typically 5-6years in the harsh environment of India.</p>

                <p>"Every Arvi UPS is carefully engineered and subjected to 12 rigorous stages of quality 
                    inspection and heatrun; ensures minimum 10years of product life - justifies the return on 
                    your high investment and also lesser cost per year."</p>

                <ul class="bullets">
                    <li>Indigenous design.</li>
                    <li>Easy to service single DSP card.</li>
                    <li>Rugged, long-life design.</li>
                    <li>Prompt & ex-stock delivery.</li>
                    <li>Power Factor Corrected, Parallel redundancy, regenerative load handling.</li>
                    <li>10years of unconditional spares support.</li>
                    <li>SNMP, MODBUS communication interface.</li>
                </ul>

                <p style="font-size: 7px; color: #666;">Esthetically designed - export compliant. Exported to Dubai, Nigeria, Philippines, Malaysia and Nepal</p>
            </div>
        </div>

        <!-- SECTION 2: Objective -->
        <div class="blue-header">2. We are different in our objective and approach</div>

        <p style="margin: 4px 0; font-size: 11px;"><strong style="border-bottom: 2px solid #c41e3a;">Quality of power matters</strong></p>

        <p style="margin: 4px 0; font-size: 11px; text-align: justify;">We believe that the <strong class="red-bold">quality of power matters significantly</strong> and the primary objective of opting for an UPS should be to protect the high investment made on the equipment and the infrastructure apart from providing backup.</p>

        <p style="margin: 4px 0; font-size: 11px; text-align: justify;">The non-linear loads pollute the quality of UPS power especially during peak-power operation; and this polluted power significantly impacts the life of the connected equipment.</p>

        <p style="margin: 4px 0; font-size: 11px; text-align: justify;">Hence unlike the conventional UPS manufacturers, Arvi's approach is to ensure premium-grade power even during worst loading condition - which ensures 100% uptime and optimum product life of the connected equipment; justifying the huge investment on the same.</p>

        <!-- PRINCIPLES -->
        <div class="blue-header">PRINCIPLES THAT WE LIVE BY</div>

        <div class="principles-box avoid-break">
            <div class="shield-icon">
                <div class="shield-placeholder">üõ°Ô∏è</div>
            </div>
            <div style="flex: 1; font-size: 11px;">
                <p style="color: #5a6a7a; font-weight: 600; margin-bottom: 3px;">‚Ä¢ Awarded as the best in-house R&D among Indian UPS manufacturers by SoftDisk</p>
                <p style="margin-bottom: 3px;">‚Ä¢ Manufacturer and exporter of industrial grade UPS for OEMs since 1998.</p>
                <p style="margin-bottom: 3px;">‚Ä¢ Using our 27 years of data and industry expertise, we are commited to proactively develop technological innovations that protect / ensure maximum retun on investment your investments in capital equipment and infrastructure.</p>
                <p style="margin-bottom: 3px;">‚Ä¢ Ensure lesser cost of ownership per year by providing premium grade quality power even during worst load conditionings.</p>
                <p style="margin-bottom: 3px;">‚Ä¢ Extending the number of years before the first breakdown (Maximise availability).</p>
                <p>‚Ä¢ Optimise the product life.</p>
            </div>
        </div>

        <!-- MANUFACTURING FACILITY -->
        <div class="blue-header">3. State-of-art manufacturing facility with Strong end to end in-house R&D team</div>

        <div class="img-row avoid-break">
            <% if (typeof isBase64 !=='undefined' && isBase64 && assetImages && assetImages['IMG-20260205-WA0007.jpg'])
                { %>
                <img src="<%= assetImages['IMG-20260205-WA0007.jpg'] %>" alt="Manufacturing 1">
                <% } else { %>
                    <img src="<%= assetsPath %>/IMG-20260205-WA0007.jpg" alt="Manufacturing 1">
                    <% } %>
                        <% if (typeof isBase64 !=='undefined' && isBase64 && assetImages &&
                            assetImages['IMG-20260205-WA0008.jpg']) { %>
                            <img src="<%= assetImages['IMG-20260205-WA0008.jpg'] %>" alt="Manufacturing 2">
                            <% } else { %>
                                <img src="<%= assetsPath %>/IMG-20260205-WA0008.jpg" alt="Manufacturing 2">
                                <% } %>
                                    <% if (typeof isBase64 !=='undefined' && isBase64 && assetImages &&
                                        assetImages['IMG-20260205-WA0006.jpg']) { %>
                                        <img src="<%= assetImages['IMG-20260205-WA0006.jpg'] %>" alt="Manufacturing 3">
                                        <% } else { %>
                                            <img src="<%= assetsPath %>/IMG-20260205-WA0006.jpg" alt="Manufacturing 3">
                                            <% } %>
        </div>

        <div class="stats-row avoid-break">
            <div class="stat">
                <div class="number">27</div>
                <div class="label"><span class="red">years</span> of industry expertise</div>
            </div>
            <div class="stat">
                <div class="number">1,26,000+</div>
                <div class="label"><span class="red">successful</span> installations</div>
            </div>
        </div>

        <!-- TECHNICAL EXPERT INSIGHT -->
        <div class="blue-header">Our technical expert's insight</div>

        <div class="quote-box avoid-break">
            <p>"We understand that eventhough the UPS continues to function and supply power without interuption, the connected equipment could fail due to failure of its power supply, any electrical / electronic component in the equipment - and one of the primary reason for such failures is quality of power ".</p>
            <p>"We understand that the cost of downtime and premature failure of the connected equipment is very costly as the investment done on the same is very high. Arvi UPS primarily serves as a virtual insurance to your high cost investment serving as a high impedance power-conditioning power source apart from being a UPS ".</p>
            <p>"Based on the load study conducted by our engineering team we have found that the quality of the UPS power determines the uninterupted process continuity and the product life of the connected equipment."</p>
            <p>"Equipment availability /uninterupted functioning is directly linked to the quality of the UPS power. Hence our primary focus is on providing UPS that supplies premium grade quality power to the connected load even during the most extreme situvations."</p>
        </div>

        <div class="motto-box avoid-break">
            <div class="motto-label">Our<br>motto</div>
            <div style="flex: 1;">
                <ul class="bullets">
                    <li>Extending the number of years before the first breakdown (Maximise availability).</li>
                    <li>Optimise the product life.</li>
                </ul>
            </div>
        </div>

        <!-- PROTECTION TABLE -->
        <div class="blue-header">How does Arvi UPS ensure zero downtime of your equipment and process</div>

        <p style="margin: 4px 0; font-size: 11px;">Arvi UPS ensure zero downtime of your equipment and process by pre-trip alerts - Empowers the user / maintenance technicians with proactive alerts to enhance UPS uptime.</p>

        <ul class="bullets">
            <li>Pre-trip alert for overtemperature on transformer and chokes.</li>
            <li>Pre-trip alert for overtemperature inside the UPS due to fan failure or high ambient temperature. Protection for output AC capacitor failure.</li>
            <li>Pre-trip alert for high crest loads (non-linear loads beyond the rated UPS capacity).</li>
            <li>Pre-trip alert for overload (The UPS handles the load surge and overload for few minutes before tripping).</li>
            <li>UPS is built through-out using fire resistant material for safety and reliability.</li>
        </ul>

        <!-- QUOTATION SECTION -->
        <div class="option-header">Option I - <%= data.optionTitle || 'PFC-XR 303' %></div>
        <div class="option-title">
            <div class="model"><%= data.optionTitle || 'PFC-XR 303' %></div>
            <div class="desc"><%= data.optionSubtitle || 'Online UPS - specially designed for laser cutting machines.' %></div>
        </div>

        <div class="tech-box avoid-break">
            <div style="font-size: 11px;"><%- data.optionDetails %></div>
        </div>

        <table class="quote-table avoid-break">
            <thead>
                <tr>
                    <th>Sl No</th>
                    <th>Description</th>
                    <th>Unit price</th>
                    <th>Qty</th>
                    <th>Value</th>
                </tr>
            </thead>
            <tbody>
                <% if (data.items && data.items.length> 0) { %>
                    <% data.items.forEach(function(item) { %>
                        <tr>
                            <td><%= item.slno || '' %></td>
                            <td style="text-align: left;"><%= item.description || '' %></td>
                            <td><%= item.unitPrice || '' %></td>
                            <td><%= item.qty || '' %></td>
                            <td><%= item.value || '' %></td>
                        </tr>
                        <% }); %>
                            <% } %>
            </tbody>
        </table>

        <!-- TOTAL SECTION -->
        <% if (data.subtotal && data.subtotal> 0) { %>
            <div class="total-section avoid-break">
                <div class="total-row">
                    <span class="total-label">Subtotal:</span>
                    <span class="total-value">‚Çπ <%= data.subtotal.toLocaleString('en-IN', {minimumFractionDigits: 2, maximumFractionDigits: 2}) %></span>
                </div>
                <div class="total-row">
                    <span class="total-label">GST (18%):</span>
                    <span class="total-value">‚Çπ <%= data.gstAmount.toLocaleString('en-IN', {minimumFractionDigits: 2, maximumFractionDigits: 2}) %></span>
                </div>
                <div class="total-row grand-total">
                    <span class="total-label">Grand Total:</span>
                    <span class="total-value">‚Çπ <%= data.grandTotal.toLocaleString('en-IN', {minimumFractionDigits: 2, maximumFractionDigits: 2}) %></span>
                </div>
            </div>
            <% } %>

                <% if (data.option2Title) { %>
                    <div class="option-header">Option II - <%= data.option2Title %></div>
                    <div class="option-title">
